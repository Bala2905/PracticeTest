pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
               withMaven(maven : 'maven_3_5_0') {
               bat "mvn clean"
               }
               
            }
        }
        stage('Test') { 
            steps {
               withMaven(maven : 'maven_3_5_0') {
               bat "mvn test"
               } 
            }
        }
        
        stage('Compile') { 
            steps {
               withMaven(maven : 'maven_3_5_0') {
               bat "mvn compile"
               } 
            }
        }
        stage('Deploy') { 
            steps {
                withMaven(maven : 'maven_3_5_0') {
               bat "mvn deploy" 
               }
            }
        }
    }
}
